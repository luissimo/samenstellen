!!!
/[if IE 8 ] <html class="ie ie8" lang="en">
/[if IE 9 ] <html class="ie ie9" lang="en">
%html{:lang => "nl"}
  / <![endif]
  %head
    = csrf_meta_tags

    -# Google Tag Manager
    :javascript
      (function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
      new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
      j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
      'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
      })(window,document,'script','dataLayer','GTM-5257WHS');
    %meta{:charset => "utf-8"}/

    %title= @page_title
    %meta{:content => "#{@meta_title}", :name => "title"}/
    %meta{:content => "#{@meta_description}", :name => "description"}/
    %meta{:content => "Tenzen.nl", :name => "author"}/

    -#  toolbar color for chrome on android
    %meta{:content => "#5777ab", :name => "theme-color"}/
    / Favicons
    %link{:href => "favicon.svg", :rel => "shortcut icon", :type => "image/x-icon"}/
    -# %link{:href => "apple-touch-icon-57x57-precomposed.png", :rel => "apple-touch-icon", :type => "image/x-icon"}/
    -# %link{:href => "apple-touch-icon-72x72-precomposed.png", :rel => "apple-touch-icon", :sizes => "72x72", :type => "image/x-icon"}/
    -# %link{:href => "apple-touch-icon-114x114-precomposed.png", :rel => "apple-touch-icon", :sizes => "114x114", :type => "image/x-icon"}/
    -# %link{:href => "apple-touch-icon-144x144-precomposed.png", :rel => "apple-touch-icon", :sizes => "144x144", :type => "image/x-icon"}/
    / Mobile Specific Metas
    %meta{:content => "width=device-width, initial-scale=1", :name => "viewport"}/
    / IE Compatibility Meta
    %meta{:content => "IE=edge", "http-equiv" => "X-UA-Compatible"}/
    / CSS
    = stylesheet_link_tag    'bootstrap.css', media: 'all'
    = stylesheet_link_tag    'style.css', media: 'all'
    = stylesheet_link_tag    'fancybox/source/jquery.fancybox.css', media: 'all'
    = stylesheet_link_tag    'check_radio/skins/square/aero.css', media: 'all'
    / Toggle Switch
    = stylesheet_link_tag    'jquery.switch.css', media: 'all'
    / Owl Carousel Assets
    = stylesheet_link_tag    'owl.carousel.css', media: 'all'
    = stylesheet_link_tag    'owl.theme.css', media: 'all'
    / Google web font
    %link{:href => "https://fonts.googleapis.com/css?family=Open+Sans:400,600,700,800,300", :rel => "stylesheet", :type => "text/css"}/
    /[if lt IE 9]
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    / Jquery

    = javascript_include_tag 'jquery-1.10.2.min.js'
    = javascript_include_tag 'jquery-ui-1.8.22.min.js'
    / Wizard
    = javascript_include_tag 'jquery.wizard.js'
    / Radio and checkbox styles
    = javascript_include_tag 'check_radio/jquery.icheck.js'
    / HTML5 and CSS3-in older browsers
    = javascript_include_tag 'modernizr.custom.17475.js'
    / Support media queries for IE8
    = javascript_include_tag 'respond.min.js'


  %body
    -# Google Tag Manager
    %noscript
      %iframe{:height => "0", :src => "https://www.googletagmanager.com/ns.html?id=GTM-5257WHS", :style => "display:none;visibility:hidden", :width => "0"}
    = render 'partials/header'
    = yield
    = render 'partials/footer'

    / CUSTOM JS
    = javascript_include_tag 'custom.js'
    / OTHER JS
    = javascript_include_tag 'jquery.validate.js'
    = javascript_include_tag 'jquery.placeholder.js'
    = javascript_include_tag 'jquery.tweet.min.js'
    = javascript_include_tag 'jquery.bxslider.min.js'
    = javascript_include_tag 'quantity-bt.js'
    = javascript_include_tag 'bootstrap.js'
    = javascript_include_tag 'retina.js'
    = javascript_include_tag 'owl.carousel.min.js'
    = javascript_include_tag 'functions.js'
    = javascript_include_tag 'jquery.validate.js'

    / Fancybox
    = javascript_include_tag 'fancybox/source/jquery.fancybox.pack.js'
    = javascript_include_tag 'fancybox/source/helpers/jquery.fancybox-media.js'
    = javascript_include_tag 'fancy_func.js'

    :javascript
      var Tawk_API=Tawk_API||{}, Tawk_LoadStart=new Date();
      (function(){
      var s1=document.createElement("script"),s0=document.getElementsByTagName("script")[0];
      s1.async=true;
      s1.src='https://embed.tawk.to/5a2c0b6cd0795768aaf8e4fa/default';
      s1.charset='UTF-8';
      s1.setAttribute('crossorigin','*');
      s0.parentNode.insertBefore(s1,s0);
      })();
